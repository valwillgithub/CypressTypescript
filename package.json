    "allure-commandline": "^2.23.1",
    "cypress": "^12.17.2",
    "esbuild": "^0.18.18",
    "mocha-allure-reporter": "^1.4.0",
    "typescript": "^5.1.6"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
